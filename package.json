    "lint": "eslint src/**/*",
    "lint:fix": "eslint src/**/* --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "isReady": "yarn run format && yarn run lint:fix"
  },
  "eslintConfig": {
    "extends": [
